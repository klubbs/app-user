{
  "editor.formatOnSave": true,
  "editor.rulers": [
    120
  ],
  "editor.wordWrapColumn": 100,
  // "editor.wordWrap": "bounded",
  "editor.codeLens": true,
  "workbench.colorTheme": "Dracula",
  "[typescript]": {
    "editor.codeLens": true
  },
  "[javascript]": {
    "editor.codeLens": true
  },
  "editor.guides.indentation": true,
  "search.useIgnoreFiles": false,
  "search.exclude": {
    "**/node_modules": false,
  },
}